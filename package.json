{
  "name": "fresko",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@7.0.0",
  "description": "A configurable cli that keeps your project deps fresh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qhello/fresko.git"
  },
  "main": "dist/cli.mjs",
  "module": "dist/cli.mjs",
  "types": "dist/cli.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "fresko": "bin/fresko.mjs"
  },
  "scripts": {
    "lint": "eslint .",
    "dev": "unbuild --stub",
    "build": "unbuild",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --tag --push && pnpm publish --no-git-checks",
    "test": "vitest"
  },
  "bugs": {
    "url": "https://github.com/qhello/fresko/issues"
  },
  "homepage": "https://github.com/qhello/fresko#readme",
  "dependencies": {
    "@antfu/ni": "^0.16.2",
    "build": "^0.1.4",
    "chalk": "^5.0.1",
    "debug": "^4.3.4",
    "deepmerge": "^4.2.2",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.4",
    "semver": "^7.3.7",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.22.0",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^17.0.31",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.11",
    "bumpp": "^7.1.1",
    "eslint": "^8.14.0",
    "fast-glob": "^3.2.11",
    "find-up": "^6.3.0",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vitest": "^0.10.0"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config"
  }
}
